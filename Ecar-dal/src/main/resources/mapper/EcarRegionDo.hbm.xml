<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Ecar.dal.model.EcarRegionDo" table="ecar_region" schema="ecar">
        <id name="regId" column="regID"/>
        <property name="name" column="name"/>
        <property name="simpleName" column="simpleName"/>
        <property name="pRegId" column="pRegID"/>
        <property name="type" column="type"/>
        <property name="lon" column="lon"/>
        <property name="lat" column="lat"/>
    </class>
</hibernate-mapping>